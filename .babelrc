{
    "presets": [
      "next/babel"
    ],
    "plugins": [
      "@babel/plugin-proposal-optional-chaining",
      "@babel/plugin-proposal-nullish-coalescing-operator",
      [
        "module-resolver",
        {
          "root": [
            "./"
          ],
          "alias": {
            "@app": "./src/app",
            "^@onr/(.+)": "./src/modules/\\1"
          }
        }
      ],
      [
        "babel-plugin-styled-components",
        {
          "ssr": true,
          "displayName": true,
          "preprocess": false
        }
      ],
      [
        "import",
        {
          "libraryName": "antd"
        }, "ant"
      ],
      [
        "import",
        {
          "libraryName": "react-feather",
          "libraryDirectory": "dist/icons"
        },
        "react-feather"
      ]
    ]
}
